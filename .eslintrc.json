{
  "extends": [
    "standard",
    "standard-react",
    "standard-jsdoc",
    "standard-latest",
    "plugin:unicorn/recommended"
  ],
  "plugins": [
    "json",
    "no-loops",
    "unicorn",
    "jsdoc"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-loops/no-loops": 2
  }
}